pico-8 cartridge // http://www.pico-8.com
version 32
__lua__
function _init()
	screen = {}
	screen.w = 64
	screen.h = screen.w
	screen.x = 128/2 - screen.w/2
	screen.y = 128/2 - screen.h/2
end

function _draw()
	cls()
	ovalfill(8,0,127-8,127,9)
	rectfill(screen.x,screen.y,screen.x+screen.w-1,screen.y+screen.h-1,6)

	palt(0,false)
	sspr(8,0,8,8,56,80,16,16)
	palt(0,true)

	for i=0,2 do
		circfill(screen.x+8+24*i,screen.y+screen.h+8+4*(i%2),4,5)
	end


	circfill(64,8,4,0)

	print("tamagotchi",44,20,8)

	print("hi there!",46,48,0)
end
