pico-8 cartridge // http://www.pico-8.com
version 32
__lua__
#include physics.p8

function _init()
	debug_mode = false
	--debug_mode = true

	screen = {}
	screen.w = 128
	screen.h = 128
	screen.x = 0
	screen.y = 0

	player = {}
	player.x = 16
	
	player.w = 7
	player.h = 7
	player.dx = 0
	player.dy = 0
	player.is_jumping = false
	if(debug_mode) then
		player.is_falling = true
		player.y = 48
	else
		player.is_falling = false
		player.y = 112
	end
	player.dir = "right"

	cam = {x=0,y=0}

	if(debug_mode) then
		state = "game"
	else
		state = "logo"
	end

	logo_timer = 120

	timer = 0

	fun = 0

	-- generate blocks
	blocks = {}
	for j=0,16 do
		for i=0,128 do
			if(solid(i*8,j*8)) then
				add(blocks,{x=i*8,y=j*8,w=8,h=8})
			end
		end
	end

	version = "0.1.1"
end

function _update()

	if(state == "logo") then

		if(timer >= logo_timer) then
			state = "game"
		end

		timer += 1

	elseif(state == "game") then

		--b_y = get_block_y(player.y+player.dy+8)

		-- update the camera
		cam = {x=player.x-64,y=player.y-64}

		if(player.x < 64) then
			cam.x = 0
		elseif(player.x >= 128*8-64) then
			cam.x = 128*8-128
		end

		if(debug_mode and btnp(4) or not debug_mode) then

			player = player_controls(player)

			player = move_actor(player, true)

			timer += 1
		end
	end
end

function _draw()
	cls()

	palt(0,false)
	palt(11,true)

	if(state == "logo") then

		camera(0,0)

		-- screen
		rectfill(0,0,127,127,6)

		-- show the roc studios logo
        sspr(32*3, 32*3, 32, 32, 32, 16, 64, 64)
        sspr(32*2, 32*3+16, 32, 16, 32, 16+64, 64, 32)

        --[[if(timer >= 30) then
			rectfill(0,0,127,127,0)
		end]]

		if(timer >= logo_timer - 60) then

			local j = timer - (logo_timer - 60)

			for i=0,flr(j/2.2)*8,8 do
				rectfill(0,i,127,i+flr(j/5),0)
			end
		end

	elseif(state == "game") then

		camera(cam.x,0)

		-- screen
		rectfill(cam.x+screen.x,screen.y,cam.x+screen.x+screen.w-1,screen.h-1,6)
		
		map(0,0,0,0,128,16)

		-- pet character
		if(player.is_jumping or player.is_falling) then
			if(player.dir == "right") then
				spr(3,player.x,player.y)
			else
				spr(3,player.x,player.y,1,1,true)
			end
		else
			if(player.dir == "right") then
				spr(1+flr(timer/15)%2,player.x,player.y)
			else
				spr(1+flr(timer/15)%2,player.x,player.y,1,1,true)
			end
		end
		
		--[[print("jumping = " .. tostr(player.is_jumping), cam.x + 12, 0, 0)
		print("falling = " .. tostr(player.is_falling), cam.x + 12, 8, 0)
		print("y = " .. tostr(player.y), cam.x + 12, 16, 0)
		print("dy = " .. tostr(player.dy), cam.x + 12, 24, 0)]]
		--print("y % 8 = " .. tostr(player.y % 8))

		--print("hi there!",cam.x+46,48,0)
		if(debug_mode) then
			rectfill(0,0,128,48,0)
			rect(player.x-player.x%8,get_block_y(player.y+player.dy+8),player.x-player.x%8+8,get_block_y(player.y+player.dy+8)+8,8)
			print("y = " .. tostr(player.y),cam.x,0,8)
			print("dy = " .. tostr(player.dy),cam.x,8,8)
			print("y + dy = " .. tostr(player.y + player.dy),cam.x,16,8)
			print("y % 8 = " .. tostr(player.y % 8),cam.x,24,8)
			print("block y = " .. tostr(b_y),cam.x,32,8)
			print("falling = " .. tostr(player.is_falling),cam.x,40,8)
			--print("hello", cam.x, 0, 8)
		end
	end

	palt(0,true)
	palt(11,false)
end
